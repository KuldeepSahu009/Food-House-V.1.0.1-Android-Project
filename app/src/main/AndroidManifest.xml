<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pvt.foodhouse">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodHouse">
        <activity android:name=".LoaderActivityTwo"></activity>
        <activity android:name=".PaymentActivity" />
        <activity android:name=".WalletActivity" />
        <activity android:name=".ErrorActivity" />
        <activity android:name=".LoaderActivityOne" />
        <activity android:name=".SupportActivity" />
        <activity android:name=".AdminVerificationActivity" />
        <activity android:name=".RateUsActivity" />
        <activity android:name=".AdminPanalActivity" />
        <activity android:name=".LocationPickerActivity" /> <!-- API for Google Map -->
        <activity android:name=".DashBoardActivity" />
        <activity android:name=".OTPActivityTwo" />
        <activity android:name=".SetPasswordActivity" />
        <activity android:name=".OTPActivity" />
        <activity android:name=".SignUpDoneActivity" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>